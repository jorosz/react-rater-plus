{
  "name": "react-rater-plus",
  "version": "0.1.1",
  "description": "A star rater in react.js with support for CSS modules and theming",
  "main": "lib/rater.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "test": "node_modules/.bin/jest --verbose",
    "build": "rm -rf lib && node_modules/.bin/webpack && cp src/rater.css lib/",
    "start": "node_modules/.bin/webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jorosz/react-rater-plus"
  },
  "keywords": [
    "react",
    "rater",
    "star-rater",
    "react-component"
  ],
  "author": "jorosz",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jorosz/react-rater-plus/issues"
  },
  "homepage": "https://github.com/jorosz/react-rater-plus",
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-css-themr": "^1.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-polyfill": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-react": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "css-loader": "^0.26.1",
    "eslint": "^2.10.2",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^5.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "postcss-cssnext": "^2.9.0",
    "postcss-loader": "^1.3.0",
    "react-addons-test-utils": "^15.0.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
